    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.4.2",
    "sass": "^1.83.4",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
